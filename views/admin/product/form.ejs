    <script>
      (() => {
            (function () {
                "use strict";

                // Select all elements with the class 'editor'
                $(".editor").each(function () {
                    let editorElement = this;

                    // Initialize ClassicEditor on each element
                    ClassicEditor.create(editorElement)
                        .then(editor => {
                            // Save the editor instance for later use
                            window.editorInstance = editor;

                            editor.model.document.on('change:data', () => {
                                const content = editor.getData();
                                
                                document.getElementById('productDescription').value = content;

                                // You can use this content for further processing
                            });
                            
                        })
                        .catch(error => {
                            // Log any errors during the editor creation process
                            console.error(error);
                        });
                });

                function setEditorContent(content) {
                    if (window.editorInstance) {
                        window.editorInstance.setData(content);
                    }
                }

                // Function to get content
                function getEditorContent() {
                    if (window.editorInstance) {
                        return window.editorInstance.getData();
                    }
                    return '';
                }

            })();
        })();

    </script>
